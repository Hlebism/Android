<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.klient2.rsa">
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".RSAMAIN">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RSACIPHER"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".RSADATA"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".RSADECIPHER"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".RSAVIEWKEYS"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".RSAVIEWCIPHERED"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".RSAVIEWDECIPHERED"
            android:label="@string/app_name">
        </activity>


    </application>

</manifest>
